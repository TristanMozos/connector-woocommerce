<odoo>
    <record id="view_tax_form" model="ir.ui.view">
        <field name="name">account.tax.form connector_woocommerce</field>
        <field name="model">account.tax</field>
        <field name="inherit_id" ref="account.view_tax_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[2]" position="inside">
                <group name="woocommerce_bindings" string="WooCommerce Bindings">
                    <field name="woocommerce_bind_ids" colspan="4" nolabel="1">
                        <tree string="WooCommerce Bindings" editable="bottom">
                            <field name="external_id"/>
                            <field name="backend_id"/>
                        </tree>
                    </field>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
