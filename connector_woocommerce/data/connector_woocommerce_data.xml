<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="ir_cron_import_sale_orders" forcecreate="True">
            <field name="name">WooCommerce - Import Sales Orders</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="connector_woocommerce.model_wc_backend" name="model_id"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
model._scheduler_import_sale_orders()
]]></field>
        </record>

        <record model="ir.cron" id="ir_cron_update_product_stock_qty" forcecreate="True">
            <field name="name">WooCommerce -  Update Stock Quantities</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="connector_woocommerce.model_wc_backend" name="model_id"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
model._scheduler_update_product_stock_qty()
]]></field>
        </record>

        <record model="ir.cron" id="ir_cron_import_product" forcecreate="True">
            <field name="name">WooCommerce -  Import Products</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="connector_woocommerce.model_wc_backend" name="model_id"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
model._scheduler_import_products()
]]></field>
        </record>

        <record model="ir.cron" id="ir_cron_force_product_stock_qty" forcecreate="True">
            <field name="name">WooCommerce -  Force Stock Quantities</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field ref="connector_woocommerce.model_wc_backend" name="model_id"/>
            <field name="state">code</field>
            <field name="code"><![CDATA[
model._scheduler_force_product_stock_qty()
]]></field>
        </record>
    </data>
</odoo>
